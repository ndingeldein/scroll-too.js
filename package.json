{
  "name": "scroll-too",
  "dependencies": {
    "jump.js": "^1.0.2"
  },
  "ava": {
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "failFast": true,
    "tap": false,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-preset-latest": "^6.22.0",
    "laravel-mix": "^0.8.7",
    "standard": "^8.6.0"
  },
  "scripts": {
    "test": "ava",
    "watch:test": "ava --watch",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "author": "Neil Dingeldein <ndingeldein@gmail.com> (http://www.neildingeldein.com)",
  "license": "MIT",
  "description": "[![Standard - JavaScript Style Guide](https://img.shields.io/badge/code%20style-standard-brightgreen.svg)](http://standardjs.com/)",
  "version": "0.0.0",
  "main": "src/scroll-too.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndingeldein/scroll-too.js.git"
  },
  "keywords": [
    "jump.js",
    "smooth",
    "scroll",
    "anchor"
  ],
  "bugs": {
    "url": "https://github.com/ndingeldein/scroll-too.js/issues"
  },
  "homepage": "https://github.com/ndingeldein/scroll-too.js#readme"
}
